<h1>Events Dashboard</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Recipient</th>
      <th scope="col">Event Id</th>
      <th scope="col">Processed</th>
      <th scope="col">Delivered</th>
    </tr>
  </thead>
  <tbody>
    <% @events.each do |event| %>
        <tr>
            <td><%= event.email %></td>
            <td><%= event.event %></td>
            <td><%= event.event %></td>
            <td><%= event.event %></td>
        </tr>
    <% end %>    
    
  </tbody>
</table>